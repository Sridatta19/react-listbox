    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "autoprefixer": "9.7.4",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "4.2.0",
    "commitizen": "4.0.3",
