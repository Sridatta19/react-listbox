{
  "name": "react-listbox",
  "version": "0.0.0-semantic-release",
  "description": "a double listbox react component",
  "main": "build/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha --compilers js:babel-core/register --recursive tests/**/*.spec.js --require tests/setup.js",
    "prebuild": "rimraf build && rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir build src",
    "build:umd": "webpack --output-filename react-listbox.js",
    "build:umd.min": "webpack --output-filename react-listbox.min.js -p",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sridatta19/react-listbox.git"
  },
  "keywords": [
    "react",
    "react-component",
    "listbox",
    "multi-select"
  ],
  "files": [
    "build",
    "dist",
    "public"
  ],
  "author": "Sridatta <dattacrew19@gmail.com> (https://twitter.com/dattacrew19)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sridatta19/react-listbox/issues"
  },
  "homepage": "https://github.com/Sridatta19/react-listbox#readme",
  "dependencies": {
    "ramda": "0.26.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "autoprefixer": "9.6.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-jsx-self": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "chai": "4.2.0",
    "commitizen": "4.0.3",
    "css-loader": "3.2.0",
    "cz-conventional-changelog": "3.0.2",
    "enzyme": "3.10.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "4.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "4.2.0",
    "ghooks": "2.0.4",
    "ignore-styles": "5.0.1",
    "json-loader": "0.5.7",
    "mocha": "6.2.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "postcss-loader": "3.0.0",
    "rimraf": "2.6.3",
    "semantic-release": "^15.13.19",
    "style-loader": "1.0.0",
    "url-loader": "2.1.0",
    "webpack": "4.39.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
